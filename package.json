{
  "name": "typed-json-transform",
  "version": "0.16.6",
  "description": "_ like library written in typescript using templated generics where reasonable",
  "homepage": "https://github.com/1e1f/typed-json-transform",
  "author": {
    "name": "Leif Shackelford",
    "email": "leif@chroma.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1e1f/typed-json-transform.git"
  },
  "bugs": {
    "url": "https://github.com/1e1f/typed-json-transform/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/1e1f/typed-json-transform/blob/master/LICENSE-MIT"
    }
  ],
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "gulp scripts",
    "test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail",
    "cover": "NODE_ENV=test nyc --report-dir coverage npm t",
    "travis": "npm run build && npm run cover"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.46",
    "@types/js-yaml": "^3.5.29",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.1",
    "js-yaml": "^3.7.0",
    "mocha": "^3.2.0",
    "typescript": "^2.1.6",
    "ts-node": "^2.0.0",
    "nyc": "^8.4.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "node_modules"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "lcov"
    ],
    "all": true
  }
}
